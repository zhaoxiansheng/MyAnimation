<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ScrollAnimView">
        <attr name="round_radius" format="dimension" />
        <attr name="round_color" format="color" />
        <attr name="text_color" format="color" />
        <attr name="text_size" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CalendarView">
        <attr name="circle_radius" format="dimension" />
        <attr name="circle_color" format="color" />
        <attr name="date_row_height" format="dimension" />
        <attr name="date_row_width" format="dimension" />
        <attr name="date_col_width" format="dimension" />
        <attr name="date_col_height" format="dimension" />
        <attr name="date_color" format="color" />
        <attr name="date_size" format="dimension" />
        <attr name="calendar_mode" format="string" />
    </declare-styleable>
</resources>